---
source: testing-tracing-opentelemetry/src/lib.rs
expression: tracing_events
---
- fields:
    message: new
  level: INFO
  span:
    http.client_ip: ""
    http.flavor: "1.1"
    http.host: ""
    http.method: GET
    http.route: "/nest/:nest_id"
    http.scheme: HTTP
    http.target: /nest/123
    http.user_agent: ""
    name: HTTP request
    otel.kind: server
    otel.name: "GET /nest/:nest_id"
    trace_id: "[trace_id:lg32]"
  spans: []
  target: "axum_tracing_opentelemetry::middleware::trace_extractor"
  timestamp: "[timestamp]"
- fields:
    message: close
    time.busy: "[duration]"
    time.idle: "[duration]"
  level: INFO
  span:
    http.client_ip: ""
    http.flavor: "1.1"
    http.host: ""
    http.method: GET
    http.route: "/nest/:nest_id"
    http.scheme: HTTP
    http.status_code: "200"
    http.target: /nest/123
    http.user_agent: ""
    name: HTTP request
    otel.kind: server
    otel.name: "GET /nest/:nest_id"
    otel.status_code: OK
    trace_id: "[trace_id:lg32]"
  spans: []
  target: "axum_tracing_opentelemetry::middleware::trace_extractor"
  timestamp: "[timestamp]"

